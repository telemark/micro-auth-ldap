{
  "name": "micro-auth-ldap",
  "description": "ldap auth microservice",
  "version": "1.5.7",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "contributors": [
    {
      "name": "Jonas Enge",
      "email": "jonas.enge@gmail.com",
      "url": "https://github.com/maccyber"
    }
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9.4"
  },
  "scripts": {
    "test": "standard && nsp check && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "dev": "micro-dev",
    "start": "micro",
    "deploy": "npm test && now && now alias",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "keywords": [
    "microservice",
    "micro",
    "ldap",
    "auth",
    "Telemark fylkeskommune"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/telemark/micro-auth-ldap.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/micro-auth-ldap/issues"
  },
  "homepage": "https://github.com/telemark/micro-auth-ldap#readme",
  "now": {
    "alias": [
      "ldap.auth.mikrotjeneste.win"
    ],
    "type": "npm",
    "env": {
      "NODE_ENV": "production"
    },
    "engines": {
      "node": "8.9.4"
    }
  },
  "devDependencies": {
    "ava": "0.25.0",
    "coveralls": "3.0.2",
    "nsp": "3.2.1",
    "micro-dev": "3.0.0",
    "nyc": "12.0.2",
    "standard": "11.0.1",
    "test-listen": "1.1.0"
  },
  "dependencies": {
    "axios": "0.18.0",
    "jsonwebtoken": "8.3.0",
    "ldap-get-user": "1.0.9",
    "ldapauth-fork": "4.0.2",
    "marked": "0.4.0",
    "micro": "9.3.0",
    "simple-encryptor": "1.4.0",
    "urlencoded-body-parser": "2.0.1",
    "winston": "2.4.3",
    "winston-papertrail": "1.0.5"
  }
}
